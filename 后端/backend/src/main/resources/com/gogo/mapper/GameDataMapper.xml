<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gogo.mapper.GameDataMapper">
    <resultMap id="GameDataResultMap" type="com.gogo.pojo.GameData">
        <result column="play_match" property="playMatch"/>
        <result column="red_cards" property="redCards"/>
        <result column="yellow_cards" property="yellowCards"/>
    </resultMap>
    <select id="selectById" resultMap="GameDataResultMap">
        SELECT *
        FROM game_data
        WHERE id = #{id}
        ;
    </select>

</mapper>